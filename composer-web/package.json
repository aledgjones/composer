{
  "name": "composer-ui",
  "version": "0.50.3",
  "description": "create an app to consume rust-generated wasm packages",
  "main": "bootstrap.ts",
  "scripts": {
    "wasm": "wasm-pack build ../composer-engine --target web",
    "wasm:release": "wasm-pack build ../composer-engine --target web --release",
    "dev": "npm run wasm && vite",
    "build": "vite build",
    "deploy": "npm run deploy:patch",
    "deploy:patch": "npm run wasm:release && npm version patch && git push",
    "deploy:minor": "npm run wasm:release && npm version minor && git push",
    "deploy:major": "npm run wasm:release && npm version major && git push"
  },
  "author": "Aled Jones <aledjones.viola@gmail.com>",
  "license": "(MIT OR Apache-2.0)",
  "devDependencies": {
    "@types/big.js": "^6.1.3",
    "@types/color": "^3.0.3",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/pretty-ms": "^5.0.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.12.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.0"
  },
  "dependencies": {
    "@mdi/js": "^6.6.96",
    "app-reset": "^1.0.2",
    "big.js": "^6.1.1",
    "classnames": "^2.3.1",
    "color": "^4.2.1",
    "composer-engine": "file:../composer-engine/pkg",
    "nanoid": "^3.3.2",
    "pretty-ms": "^7.0.1",
    "pullstate": "^1.23.0",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  }
}
